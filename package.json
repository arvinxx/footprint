{
  "name": "arvinxx-footprint",
  "version": "1.1.3",
  "private": true,
  "description": "空谷的足迹",
  "repository": "https://github.com/arvinxx/footprint.git",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "-------": "",
    "dev": "npm run start:dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "dev:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "------- ": "",
    "api:dev": "vercel dev",
    "-------  ": "",
    "build:analyze": "cross-env ANALYZE=1 umi build",
    "-------   ": "",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "-------    ": "",
    "release": "semantic-release",
    "-------     ": "",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "tsc": "tsc --noEmit",
    "-------      ": "",
    "postinstall": "umi g tmp"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.6.2",
    "@ant-design/pro-form": "^1.3.0",
    "@ant-design/pro-layout": "^6.14.4",
    "@ant-design/pro-table": "^2.30.2",
    "@umijs/route-utils": "^1.0.33",
    "antd": "^4.9.2",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "nprogress": "^0.2.0",
    "omit.js": "^2.0.2",
    "qs": "^6.9.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.4",
    "tailwindcss": "^2.0.2",
    "umi": "^3.3.9",
    "umi-request": "^1.3.5",
    "use-merge-value": "^1.0.1",
    "zotero-api-client": "^0.33.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.4",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.3.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/test": "^3.3.9",
    "@umijs/yorkie": "^2.0.3",
    "chalk": "^4.0.0",
    "commitlint": "^11.0.0",
    "commitlint-config-gitmoji": "^2.2.2",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "semantic-release": "^17.4.0",
    "semantic-release-config-gitmoji-module": "^1.1.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2",
    "umi-plugin-tailwindcss": "^3.0.3",
    "vercel": "^21.3.1"
  }
}
